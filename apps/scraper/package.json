{
  "name": "scraper",
  "version": "1.5.0",
  "description": "A modern scraper for the uOttawa public course registry",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "db:cleanup": "node dist/cleanup.js",
    "lint": "eslint .",
    "scrape:courses": "node dist/main.js",
    "scrape:subjects": "node dist/scrape/scrapeAvailableSubjects.js",
    "scrape:terms": "node dist/scrape/scrapeAvailableTerms.js"
  },
  "dependencies": {
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "fetch-cookie": "^3.1.0",
    "neverthrow": "^8.2.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "puppeteer-core": "^24.29.1"
  },
  "devDependencies": {
    "@repo/db": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^24.10.0",
    "domhandler": "^5.0.3",
    "eslint": "catalog:",
    "typescript": "catalog:"
  }
}
