{
  "name": "uenroll-scraper",
  "version": "1.6.1",
  "description": "A modern scraper for the uOttawa public course registry - Cloudflare Workers edition",
  "type": "module",
  "scripts": {
    "check-types": "wrangler types && tsc --noEmit",
    "db:cleanup": "bun src/cleanup.ts",
    "deploy": "wrangler deploy --env dev && wrangler deploy --env prod",
    "deploy:dev": "wrangler deploy --env dev",
    "deploy:prod": "wrangler deploy --env prod",
    "dev": "wrangler dev --env dev",
    "lint": "eslint ."
  },
  "dependencies": {
    "cheerio": "^1.1.2",
    "neverthrow": "^8.2.0"
  },
  "devDependencies": {
    "@repo/db": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^25.0.9",
    "domhandler": "^5.0.3",
    "dotenv": "^17.2.3",
    "eslint": "catalog:",
    "typescript": "catalog:",
    "wrangler": "^4.59.1"
  }
}
