{
  "name": "uenroll",
  "private": true,
  "license": "CC-BY-NC-SA-4.0",
  "author": "Raman Gupta",
  "scripts": {
    "build": "turbo run build",
    "check-types": "turbo run check-types",
    "ci:format": "prettier --check \"**/*.{js,jsx,ts,tsx,md}\"",
    "ci:format:packagejson": "sort-package-json --check ./**/package.json --ignore ./**/node_modules",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:packagejson": "sort-package-json ./**/package.json --ignore ./**/node_modules",
    "lint": "turbo run lint"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.5",
    "eslint": "catalog:",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "sort-package-json": "^3.3.1",
    "tsup": "^8.5.0",
    "turbo": "^2.5.4",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=22"
  }
}
