{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "git": {
    "deploymentEnabled": {
      "changeset-release/main": false,
      "docs/*": false
    }
  },
  "installCommand": "curl -fsSL https://bun.sh/install | bash && ~/.bun/bin/bun install --frozen-lockfile",
  "buildCommand": "~/.bun/bin/bun build --filter @repo/env && ~/.bun/bin/bun build --filter @repo/db && ~/.bun/bin/bun --bun next build",
  "bunVersion": "1.x"
}
